                                <div class="panel-body">
                                    Page 2
                                </div>
                            </div>
                        </div>

                        <!-- "Pages" creation -->
                        <div class="panel panel-primary">
                            <div class="panel-heading" role="tab" id="headingPagesNew">
                                <h4 class="panel-title">
                                    <a role="button" data-toggle="collapse" data-parent="#accordionPages" href="#collapsePagesNew" aria-expanded="true" aria-controls="collapsePagesNew">
                                        <span class="glyphicon glyphicon-plus"></span> New page
                                    </a>
                                </h4>
                            </div>
                            <div id="collapsePagesNew" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingPagesNew">
                                <div class="panel-body">
                                    <form class="form-horizontal" method="POST">
                                        <!-- Form Page title -->
                                        <div class="form-group">
                                            <label for="page-name" class="col-sm-2 control-label">Page name</label>
                                            <div class="col-sm-10">
                                                <input name="page-name" type="text" class="form-control" id="page-name" placeholder="Page name">
                                            </div>
                                        </div>
                                        <!-- Form Page HTML content -->
                                        <div class="form-group">
                                            <label for="page-content" class="col-sm-2 control-label">Page content</label>
                                            <div class="col-sm-10">
                                                <textarea name="page-content" class="form-control tinymce" id="page-content">Page content</textarea>
                                            </div>
                                        </div>
                                        <!-- Form Page more... -->
                                        <div class="form-group">
                                            <div class="col-sm-10 col-sm-offset-2">
                                                <a class="btn btn-default" role="button" data-toggle="collapse" href="#collapsePageMore" aria-expanded="false" aria-controls="collapsePageMore">
                                                    More...
                                                </a>
                                            </div>
                                        </div>
                                        <div class="collapse" id="collapsePageMore">
                                            <div class="form-more">
                                                <!-- Form Page URL -->
                                                <div class="form-group">
                                                    <label for="page-url" class="col-sm-2 control-label">URL</label>
                                                    <div class="col-sm-10">
                                                        <input name="page-url" type="text" class="form-control" id="page-url" placeholder="page-url" value="page-1">
                                                    </div>
                                                </div>
                                                <!-- Form Page HTML Head additions -->
                                                <div class="form-group">
                                                    <label for="page-html-head" class="col-sm-2 control-label"><code>&lt;head&gt;</code></label>
                                                    <div class="col-sm-10">
                                                        <textarea name="page-html-head" class="form-control" id="page-html-head"></textarea>
                                                    </div>
                                                </div>
                                                <!-- Form Page HTML footer additions -->
                                                <div class="form-group">
                                                    <label for="page-html-footer" class="col-sm-2 control-label"><code>&lt;footer&gt;</code></label>
                                                    <div class="col-sm-10">
                                                        <textarea name="page-html-footer" class="form-control" id="page-html-footer"></textarea>
                                                    </div>
                                                </div>
                                                <!-- Form Page CSS additions -->
                                                <div class="form-group">
                                                    <label for="page-css" class="col-sm-2 control-label">CSS</label>
                                                    <div class="col-sm-10">
                                                        <textarea name="page-css" class="form-control" id="page-css"></textarea>
                                                    </div>
                                                </div>
                                                <!-- Form Page JS additions -->
                                                <div class="form-group">
                                                    <label for="page-js" class="col-sm-2 control-label">JS</label>
                                                    <div class="col-sm-10">
                                                        <textarea name="page-css" class="form-control" id="page-js"></textarea>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- Form Page submit -->
                                        <div class="form-group">
                                            <div class="col-sm-10 col-sm-offset-2">
                                                <button type="submit" class="btn btn-primary" role="submit">
                                                    Save
                                                </button>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div><!-- ./ "Pages" creation -->
                    </div><!-- ./ "Pages" sections parent -->
                </div>
            </div><!-- ./ "Pages" content -->
        </div><!-- ./ Section "Pages" -->

        <!-- Section "Categories" -->
        <div class="panel panel-default">
            <div class="panel-heading" role="tab" id="headingCategories">
                <h4 class="panel-title">
                    <a class="collapsed" role="button" data-toggle="collapse" data-parent="#accordionParent" href="#collapseCategories" aria-expanded="false" aria-controls="collapseCategories">
                        Categories
                    </a>
                </h4>
            </div>
            <div id="collapseCategories" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingCategories">
                <div class="panel-body">
